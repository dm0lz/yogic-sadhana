
<div class="container">
  <div class="row">

    <%= render partial: "edit_form", locals: {media: @media} %>
    <%= link_to t('medias.buttons.back_to_media_description'), backend_media_path(@media), class: "row btn btn-success" %>
    <%= link_to t('medias.buttons.back_to_medias_list'), @media.mediumable_type == "Theory" ? backend_theory_medias_path(@media.mediumable) : backend_practice_medias_path(@media.mediumable), class: "row btn btn-info", style: "margin-left:20px" %>

  </div>
</div>
